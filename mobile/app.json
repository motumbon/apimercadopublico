{
  "expo": {
    "name": "Seguimiento OC",
    "slug": "seguimiento-oc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#1e40af"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seguimientooc.app"
    },
    "android": {
      "package": "com.seguimientooc.app",
      "permissions": [
        "INTERNET",
        "POST_NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1e40af"
      }
    },
    "web": {},
    "extra": {
      "apiUrl": "https://web-production-fe1d1.up.railway.app/api",
      "eas": {
        "projectId": "4c0421e4-574a-4efd-9d5d-595c9c00a1e6"
      }
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#1e40af",
          "mode": "production"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#1e40af",
      "androidMode": "default",
      "androidCollapsedTitle": "Seguimiento OC"
    },
    "owner": "motumbon"
  },
  "owner": "motumbon"
}
